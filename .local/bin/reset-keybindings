#!/usr/bin/env bash

gsettings set org.gnome.desktop.wm.preferences workspace-names "['α', 'β', 'γ', 'δ', 'ε', 'ζ', 'η', 'θ', 'ι', 'κ']"
gsettings set org.gnome.desktop.datetime automatic-timezone "true"
gsettings set org.gnome.desktop.wm.preferences num-workspaces "10"
gsettings set org.gnome.mutter dynamic-workspaces "false"
gsettings set org.gnome.mutter edge-tiling "false"
gsettings set org.gnome.mutter workspaces-only-on-primary "false"

gsettings reset-recursively org.gnome.desktop.wm.keybindings
gsettings reset-recursively org.gnome.mutter.keybindings
gsettings reset-recursively org.gnome.mutter.wayland.keybindings
gsettings reset-recursively org.gnome.settings-daemon.plugins.media-keys
gsettings reset-recursively org.gnome.shell.keybindings

gsettings set org.gnome.shell.extensions.dash-to-dock hot-keys false
for i in {1..10}; do
  gsettings set org.gnome.shell.extensions.dash-to-dock app-hotkey-"$i" "@as []"
  gsettings set org.gnome.shell.extensions.dash-to-dock app-shift-hotkey-"$i" "@as []"
  gsettings set org.gnome.shell.extensions.dash-to-dock app-ctrl-hotkey-"$i" "@as []"
done

for i in {1..9}; do
  gsettings set org.gnome.shell.keybindings switch-to-application-"$i" "@as []"       # disable Super+1-9
  gsettings set org.gnome.shell.keybindings open-new-window-application-"$i" "@as []" # disable Super+Ctrl+1-9
done

gsettings set org.gnome.desktop.wm.keybindings maximize "@as []"                           # disable Super+Up
gsettings set org.gnome.desktop.wm.keybindings minimize "['<Super>c']"                     # disable Super+h
gsettings set org.gnome.desktop.wm.keybindings unmaximize "['<Alt>F5']"                    # disable Super+Down
gsettings set org.gnome.mutter.keybindings toggle-tiled-left "@as []"                      # disable Super+Left
gsettings set org.gnome.mutter.keybindings toggle-tiled-right "@as []"                     # disable Super+Right
gsettings set org.gnome.mutter.wayland.keybindings restore-shortcuts "['<Super>r']"        # disable Super+Escape
gsettings set org.gnome.settings-daemon.plugins.media-keys screensaver "['<Super>Escape']" # disable Super+l
gsettings set org.gnome.shell.keybindings toggle-message-tray "['<Super>m']"               # disable Super+v

gsettings set org.gnome.desktop.wm.keybindings close "['<Alt>F4','<Super>q']"
gsettings set org.gnome.settings-daemon.plugins.media-keys home "['<Super>f']"
gsettings set org.gnome.settings-daemon.plugins.media-keys terminal "['<Super>t']"
gsettings set org.gnome.settings-daemon.plugins.media-keys www "['<Super>b']"

dconf write /org/gnome/settings-daemon/plugins/media-keys/custom-keybindings "['/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/editor/','/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/clipboard/']"
dconf write /org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/editor/binding "'<Super>e'"
dconf write /org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/editor/command "'/home/lmurarotto/.appimage/neovide_fdbd577265105bef4f76e497f8c22e76.AppImage'"
dconf write /org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/editor/name "'Neovide'"
dconf write /org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/clipboard/binding "'<Super>v'"
dconf write /org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/clipboard/command "'findhist'"
dconf write /org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/clipboard/name "'Clipboard'"

dconf write /org/gnome/shell/extensions/tilingshell/move-window-down "['<Control><Super>Down','<Control><Super>j']"
dconf write /org/gnome/shell/extensions/tilingshell/move-window-left "['<Control><Super>Left','<Control><Super>h']"
dconf write /org/gnome/shell/extensions/tilingshell/move-window-right "['<Control><Super>Right','<Control><Super>l']"
dconf write /org/gnome/shell/extensions/tilingshell/move-window-up "['<Control><Super>Up','<Control><Super>k']"

dconf write /org/gnome/shell/extensions/focus-changer/focus-down "['<Super>Down','<Super>j']"
dconf write /org/gnome/shell/extensions/focus-changer/focus-left "['<Super>Left','<Super>h']"
dconf write /org/gnome/shell/extensions/focus-changer/focus-right "['<Super>Right','<Super>l']"
dconf write /org/gnome/shell/extensions/focus-changer/focus-up "['<Super>Up','<Super>k']"

dconf reset -f /org/gnome/shell/extensions/space-bar/shortcuts/
dconf write /org/gnome/shell/extensions/space-bar/shortcuts/enable-move-to-workspace-shortcuts true
dconf write /org/gnome/shell/extensions/space-bar/shortcuts/enable-activate-workspace-shortcuts true
dconf write /org/gnome/shell/extensions/space-bar/shortcuts/activate-empty-key "@as []"
dconf write /org/gnome/shell/extensions/space-bar/shortcuts/activate-previous-key "@as []"
dconf write /org/gnome/shell/extensions/space-bar/shortcuts/open-menu "@as []"
dconf write /org/gnome/shell/extensions/space-bar/shortcuts/move-workspace-left "['<Super>bracketleft']"
dconf write /org/gnome/shell/extensions/space-bar/shortcuts/move-workspace-right "['<Super>bracketright']"

gsettings set org.gnome.desktop.wm.keybindings move-to-monitor-down "['<Super><Shift>Down','<Super><Shift>j']"
gsettings set org.gnome.desktop.wm.keybindings move-to-monitor-left "['<Super><Shift>Left','<Super><Shift>h']"
gsettings set org.gnome.desktop.wm.keybindings move-to-monitor-right "['<Super><Shift>Right','<Super><Shift>l']"
gsettings set org.gnome.desktop.wm.keybindings move-to-monitor-up "['<Super><Shift>Up','<Super><Shift>k']"

gsettings set org.gnome.desktop.wm.keybindings move-to-workspace-down "['<Control><Shift><Alt>Down', '<Control><Shift><Alt>j']"
gsettings set org.gnome.desktop.wm.keybindings move-to-workspace-left "['<Super><Shift>Page_Up', '<Super><Shift><Alt>Left', '<Control><Shift><Alt>Left', '<Control><Shift><Alt>h', '<Super><Shift><Alt>h']"
gsettings set org.gnome.desktop.wm.keybindings move-to-workspace-right "['<Super><Shift>Page_Down', '<Super><Shift><Alt>Right', '<Control><Shift><Alt>Right', '<Super><Shift><Alt>l', '<Control><Shift><Alt>l']"
gsettings set org.gnome.desktop.wm.keybindings move-to-workspace-up "['<Control><Shift><Alt>Up', '<Control><Shift><Alt>k']"

gsettings set org.gnome.desktop.wm.keybindings switch-to-workspace-down "['<Control><Alt>Down', '<Control><Alt>j']"
gsettings set org.gnome.desktop.wm.keybindings switch-to-workspace-left "['<Super>Page_Up', '<Super><Alt>Left', '<Control><Alt>Left', '<Super><Alt>h', '<Control><Alt>h']"
gsettings set org.gnome.desktop.wm.keybindings switch-to-workspace-right "['<Super>Page_Down', '<Super><Alt>Right', '<Control><Alt>Right', '<Super><Alt>l', '<Control><Alt>l']"
gsettings set org.gnome.desktop.wm.keybindings switch-to-workspace-up "['<Control><Alt>Up', '<Control><Alt>k']"

gsettings set org.gnome.shell.keybindings shift-overview-down "['<Super><Alt>Down', '<Super><Alt>j']"
gsettings set org.gnome.shell.keybindings shift-overview-up "['<Super><Alt>Up', '<Super><Alt>k']"

